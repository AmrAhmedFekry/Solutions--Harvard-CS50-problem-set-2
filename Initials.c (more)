  #include <cs50.h>
#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main (void){
    
    string s = get_string();
    int n = strlen(s) ; 
    
    for ( int i = 0 ;i<n ;  i++){
     if(!isalpha(s[i]) && !isalpha(s[i+1])){
      
        }else{
        if(!isalpha(s[i])){
            printf("%c",toupper(s[i+1]));
        }
        
      }
    
    
    
}

}
